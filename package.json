{
  "name": "vipo-agents",
  "private": true,
  "version": "0.1.0",
  "scripts": {
    "dev": "cross-env NODE_ENV=development next dev -p 3001 -H 0.0.0.0",
    "build": "next build",
    "start": "next start",
    "lint": "eslint .",
    "postinstall": "node scripts/postinstall.js",
    "test:api": "vitest run",
    "test:ui": "playwright test",
    "verify": "node scripts/smoke-verify.mjs",
    "report": "node scripts/report/collect.js",
    "ci:all": "cross-env NODE_ENV=test npm run test:api && npm run test:ui && npm run verify && npm run report",
    "reset-admin": "cross-env NODE_ENV=development node -r dotenv/config scripts/reset-admin.js",
    "seed-users": "cross-env NODE_ENV=development node -r dotenv/config scripts/seed-users.js",
    "seed-demo": "cross-env NODE_ENV=development node scripts/seed-demo-data.mjs",
    "clear-demo": "cross-env NODE_ENV=development node scripts/clear-demo-data.mjs"
  },
  "dependencies": {
    "bcrypt": "^6.0.0",
    "bcryptjs": "2.4.3",
    "chart.js": "^4.5.1",
    "cloudinary": "^2.8.0",
    "clsx": "2.1.1",
    "cookie": "0.6.0",
    "dayjs": "1.11.13",
    "jose": "^6.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongodb": "6.8.0",
    "mongoose": "^8.19.2",
    "next": "14.2.5",
    "node-fetch": "^3.3.2",
    "pdfkit": "^0.17.2",
    "qrcode": "1.5.4",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "sharp": "0.33.5",
    "uuid": "9.0.1",
    "zod": "3.23.8"
  },
  "devDependencies": {
    "@playwright/test": "^1.56.1",
    "autoprefixer": "10.4.19",
    "cross-env": "^10.1.0",
    "dotenv": "^17.2.3",
    "eslint": "^8.57.0",
    "eslint-config-next": "14.2.5",
    "postcss": "8.4.47",
    "supertest": "^7.1.4",
    "tailwindcss": "3.4.13",
    "vitest": "^4.0.5"
  }
}
