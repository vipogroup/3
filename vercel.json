{
  "buildCommand": "npm run build",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "framework": "nextjs",
  "outputDirectory": ".next",
  "regions": ["iad1"],
  "crons": [
    {
      "path": "/api/internal/push/agent-digest",
      "schedule": "0 18 * * *"
    },
    {
      "path": "/api/internal/push/group-buy",
      "schedule": "0 10 * * *"
    },
    {
      "path": "/api/internal/push/cron",
      "schedule": "*/15 * * * *"
    }
  ]
}
